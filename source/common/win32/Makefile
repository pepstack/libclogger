########################################################################

.PHONY: all clean

all:
	@echo "TODO: $(CURDIR)"

clean:
	@/usr/bin/find . -type f -name '*.o' | xargs -I {} sh -c "rm -f {}"
	@echo "[clean] done: $(CURDIR)"
