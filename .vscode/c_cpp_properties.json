{
    "configurations": [
        {
            "name": "mingw64 build config",
            "windowsSdkVersion": "10.0.18362.0",
            "compilerPath": "${env:MSYS64_ROOT_BASH}/mingw64/bin/gcc.exe",
            "includePath": [
                "${env:MSYS2_HOME}/mingw64/include",
                "${workspaceFolder}/source/**",
                "${workspaceFolder}/deps/libclogger/include/**",
                "${workspaceFolder}/deps/pthreads-w32/Pre-built.2/include/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "__MINGW64__"
            ],
            "intelliSenseMode": "gcc-x64"
        },
        {
            "name": "linux64 build config",
            "compilerPath": "/usr/bin/gcc",
            "includePath": [
                "/usr/include",
                "/usr/local/include",
                "${workspaceFolder}/source/**"
            ],
            "defines": [
                "_DEBUG"
            ],
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}