  "version": "0.2.0",
  "configurations": [
	{
	  "name": "(gdb) debug",
	  "type": "cppdbg",
	  "request": "launch",
	  "program": "${workspaceFolder}/test_clogger.exe",
	  "targetArchitecture": "x64",
	  "args": [],
	  "stopAtEntry": true,
	  "cwd": "${workspaceFolder}",
	  "environment": [],
	  "externalConsole": true,
	  "MIMode": "gdb",
	  "miDebuggerPath": "gdb.exe",
	  "preLaunchTask": "make",
	  "setupCommands": [
		{
		  "description": "enable pretty printing for gdb",
		  "text": "-enable-pretty-printing",
		  "ignoreFailures": true
		}
	  ]
	}
  ]
}