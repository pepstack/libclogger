    "version": "0.2.0",
    "configurations": [
        {
            "name": "mingw-gdb",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/logapp.exe",
            "targetArchitecture": "x64",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "gdb.exe",
            "preLaunchTask": "mingw-build-debug",
            "setupCommands": [
                {
                    "description": "gdb pretty",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "linux-gdb",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/logapp.exe",
            "targetArchitecture": "x64",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "preLaunchTask": "linux-build-debug",
            "setupCommands": [
                {
                    "description": "gdb pretty",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}