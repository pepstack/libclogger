    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) mingw64 debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${env:WORKSPACE_ROOT_BASH}/${workspaceFolderBasename}/test_clogger",
            "targetArchitecture": "x64",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${env:WORKSPACE_ROOT_BASH}/${workspaceFolderBasename}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "${env:MSYS64_ROOT_BASH}/usr/bin/gdb.exe",
            "preLaunchTask": "mingw-make-debug-x64",
            "setupCommands": [
                {
                    "description": "enable pretty printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
		{
            "name": "(gdb) linux64 debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/test_clogger",
            "targetArchitecture": "x64",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "preLaunchTask": "linux-make-debug-x64",
            "setupCommands": [
                {
                    "description": "enable pretty printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}